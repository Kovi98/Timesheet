@model string

@if (!String.IsNullOrEmpty(Model))
{
    var statusMessageClass = Model.StartsWith("Error") ? "danger" : "success";
    if (Model.StartsWith("Error"))
    {
        <script>
            toastr.error("@Model");
        </script>
    }
    else
    {
        <script>
            toastr.success("@Model");
        </script>
    }
}